{
    "name": "typescript-transform",
    "version": "0.8.3",
    "description": "A tool to generate typescript from JS",
    "main": "index.js",
    "author": "Zoe Carver (@zoecarver)",
    "license": "Apache-2.0",
    "scripts": {
        "format": "prettier --write '*.{js,json,ts,yml}'",
        "format:check": "prettier --check '*.{js,json,ts,yml}'"
    },
    "bin": {
        "typescript-transform": "./index.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/zoecarver/typescript-transform"
    },
    "dependencies": {
        "@babel/core": "7.5.0",
        "@babel/preset-typescript": "^7.3.3",
        "commander": "^2.20.0",
        "readline-sync": "^1.4.9"
    }
}
